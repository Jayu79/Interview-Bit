    "#Find Fibonacci\n",
    "def fibo(N):\n",
    "    if(N==1 or N==2):\n",
    "        return 1\n",
    "#     print(N)\n",
    "    return fibo(N-2)+fibo(N-1)\n",
    "fibo(4)"
   ]
