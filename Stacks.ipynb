   "source": [
    "#Balanced Paranthesis\n",
    "A = \")((())\"\n",
    "l = []\n",
    "if(A[0]==')'):\n",
    "    print(0)\n",
    "for i in range(len(A)):\n",
    "    if(A[i]=='('):\n",
    "        l.append('(')\n",
    "    else:\n",
    "        if(len(l)>0):\n",
    "            l.pop()\n",
    "if(len(l)==0):\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
   ]
