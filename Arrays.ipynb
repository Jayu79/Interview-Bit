   "source": [
    "#Max Sum Contiguous Subarray\n",
    "A = [-2, 1, -3, 4, -1, 2, 1, -5, 4]\n",
    "S = 0\n",
    "ans = float('-inf')\n",
    "for i in range(len(A)):\n",
    "    S = S + A[i]\n",
    "    ans = max(ans,S)\n",
    "    if(S<0):\n",
    "        S = 0\n",
    "print(ans)"
   ]
