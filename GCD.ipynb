    "#Greatest Common Divisor\n",
    "def gcd(A,B):\n",
    "#     print(A,B)\n",
    "    if(B==0):\n",
    "        return A\n",
    "    return gcd(B,A%B)\n",
    "gcd(4,6)"
   ]
